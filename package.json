{
  "name": "merger-revamped",
  "version": "1.0.0",
  "description": "Application to watch movies together",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "merger",
    "dmg": {
      "title": "${productName} ${version}"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "author": "Padkiyo.inc Teach",
  "license": "ISC",
  "devDependencies": {
    "electron": "^26.4.2",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "events": "^3.3.0"
  }
}
